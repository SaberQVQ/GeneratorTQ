<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.testCode.dao.ButtonDao">

    <resultMap id="BaseResultMap" type="com.example.testCode.entity.Button">
        <id column="id" jdbcType="VARCHAR2" property="id"/> 
        <result column="key" property="key"/> 
        <result column="value" property="value"/> 
        <result column="isDeleted" property="isDeleted"/> 
        <result column="createTime" property="createTime"/> 
        <result column="updateTime" property="updateTime"/> 

        
        
    </resultMap>

    <sql id="BaseColumns">
        ID,
        KEY,
        VALUE,
        IS_DELETED,
        CREATE_TIME,
        UPDATE_TIME
    </sql>

    <select id="listByQuery" resultMap="BaseResultMap">
        SELECT
        <include refid="BaseColumns" />
        FROM TP_USER
        <where>
        </where>
    </select>




</mapper>